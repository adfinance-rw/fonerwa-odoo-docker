                    <record id="l10n_rw_tax_report_taxable_sales_a" model="account.report.line">
                        <field name="name">A (Exempt)</field>
                        <field name="code">RW_00</field>
                        <field name="expression_ids">
                            <record id="l10n_rw_tax_report_taxable_sales_a_tag" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">tax_tags</field>
                                <field name="formula">00</field>
                            </record>
                        </field>
                    </record>
                    <record id="l10n_rw_tax_report_taxable_sales_b" model="account.report.line">
